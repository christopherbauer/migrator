{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Parse Sourcefiles",
			"program": "${workspaceFolder}/example/index.ts",
			"preLaunchTask": "tsc: build - example/tsconfig.json",
			"sourceMaps": true,
			"smartStep": true,
			"internalConsoleOptions": "openOnSessionStart",
			"outFiles": [
				"${workspaceFolder}/example/dist/*.js",
				"!**/node_modules/**"
			]
		}
	]
}
